<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deinspection">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DEInspection">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewCar2Activity" />
        <activity android:name=".NewCarActivity" />
        <activity android:name=".CarActivity" />
        <activity android:name=".EditDetailsActivity"/>
        <activity android:name=".CarDetailsHelpActivity" />
        <activity android:name=".CarDetailsInfoActivity" />
    </application>

</manifest>